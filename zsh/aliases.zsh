alias reload!='. ~/.zshrc'

alias cls='clear' # Good 'ol Clear Screen command

# Easier navigation: .., ..., ...., .....
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ll='ls -la'
alias h="history"
alias tailf='tail -f'
alias ip="dig +short myip.opendns.com @resolver1.opendns.com"
alias localip="ipconfig getifaddr en0"
